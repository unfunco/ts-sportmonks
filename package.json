{
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "dependencies": {
    "typed-rest-client": "^1.8.6"
  },
  "description": "Sportmonks API client library for TypeScript.",
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/unfunco/ts-sportmonks",
  "keywords": [
    "client",
    "football",
    "javascript",
    "sportmonks",
    "soccer",
    "typescript"
  ],
  "license": "Apache-2.0",
  "name": "@unfunco/ts-sportmonks",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ]
    ]
  },
  "repository": "github:unfunco/ts-sportmonks",
  "scripts": {
    "build": "pika build",
    "clean": "rimraf pkg",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "prepack": "npm run build",
    "prettier": "prettier --write './**/*.{ts,tsx,js,jsx,css}'"
  },
  "version": "0.0.0-development"
}
